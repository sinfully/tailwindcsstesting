{
  "scripts": {
    "serve": "npx tailwindcss -i ./public/tailwind.css -o ./public/build/tailwind.css --watch && npm run live",
    "development": "cross-env NODE_ENV=development postcss public/tailwind.css -o public/build/tailwind.css",
    "production": "cross-env NODE_ENV=production postcss public/tailwind.css -o public/build/tailwind.css",
    "start": "npm run serve",
    "live": "live-server ./public"
  },
  "dependencies": {
    "autoprefixer": "10.4.8",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cssnano": "5.1.12",
    "flowbite": "^2.2.1",
    "live-server": "^1.1.0",
    "postcss": "^8.4.33",
    "postcss-cli": "10.0.0",
    "tailwindcss": "3.1.8"
  },
  "keywords": [],
  "name": "tailwindcss",
  "description": "tailwindcss with html"
}
